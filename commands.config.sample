[[command]]
COMMAND_GROUP = "Server status"
NAME = "server_date"
LABEL = "Check the server date"
EXEC = "date"

[[command]]
COMMAND_GROUP = "Server status"
NAME = "server_uptime"
LABEL = "Check the server uptime"
EXEC = "uptime"

[[command]]
COMMAND_GROUP = "Server status"
NAME = "server_memory"
LABEL = "Check the server memory"
EXEC = "free"

[[command]]
COMMAND_GROUP = "Server status"
NAME = "server_kernel_log"
LABEL = "Check the server's kernel log'"
EXEC = "dmesg"